t=int(input())
for _ in range(t):
    n=int(input())
    if n==1:
        print("no")
    else:
        i=2
        flag=0
        while(i*i  <= n):
            if n%i == 0:
                flag=1 
                break
            i+=1
        if flag == 0:
            print("yes")
        else:
            print("no")
